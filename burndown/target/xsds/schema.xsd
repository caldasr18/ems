		<xsd:sequence>
			<xsd:element name="quantity" type="xsd:long" maxOccurs="1"
				minOccurs="1"></xsd:element>
			<xsd:element name="sum" type="xsd:long" maxOccurs="1"
				minOccurs="1"></xsd:element>
			<xsd:element name="day_of_week" type="xsd:string"
				maxOccurs="unbounded" minOccurs="1"></xsd:element>			<xsd:element name="order_amounts" type="xsd:long"				maxOccurs="unbounded" minOccurs="1"></xsd:element>			<xsd:element name="portal_id" type="xsd:long" maxOccurs="unbounded"				minOccurs="1"></xsd:element>			<xsd:element name="time_of_day" type="xsd:dateTime"				maxOccurs="unbounded" minOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>


