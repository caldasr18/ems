<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">


	<!-- Document : billpay_request_response.xsd Created on : 8. April 2013 
		Author : rrodrigues Description : initial version version : 0.1.1 changelog: 
		0.1.1 (2012-09-14, bohnc) removed Umlaute, fixed namespace prefix xs to xsd; 
		dataResponseType moved to global complex type (better for code-generation) -->
	<xsd:element name="dataRequest" type="dataRequestType" />

	<xsd:element name="dataResponse" type="dataResponseType">
		<!-- <xsd:complexType> <xsd:choice> <xsd:element name="dataMessage" type="dataMessageType"/> 
			<xsd:element name="errorMessage" type="errorMessageType" /> </xsd:choice> 
			</xsd:complexType> -->
	</xsd:element>

	<xsd:complexType name="dataResponseType">
		<xsd:choice>
			<xsd:element name="dataMessage" type="dataMessageType" />
			<xsd:element name="errorMessage" type="errorMessageType" />
		</xsd:choice>
	</xsd:complexType>


	<!-- ############################## request Message ############################## -->

	<xsd:complexType name="dataRequestType">
		<xsd:sequence>


			<xsd:element name="company_details" type="CompanyDetailsType"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="billing_address_data" type="BillingAddressDataType"
				minOccurs="1" maxOccurs="1" />
			<xsd:element name="shipping_address_data" type="ShippingAddressDataType"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="customer_attributes" type="CustomerAttributesType"
				minOccurs="1" maxOccurs="1" />
			<xsd:element name="customer_bank_data" type="CustomerBankDataType"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="customer_limit" type="CustomerLimitType"
				minOccurs="1" maxOccurs="1" />
			<xsd:element name="rating_provider" type="RatingProviderType"
				minOccurs="1" maxOccurs="unbounded" />
			<xsd:element name="rating_provider_global_settings" type="RatingProviderGlobalSettingsType"
				maxOccurs="unbounded" minOccurs="1">
			</xsd:element>
			<xsd:element name="regis24_moma_box" type="Regis24MomaBoxType"
				maxOccurs="1" minOccurs="0">
			</xsd:element>
			<xsd:element name="device_fingerprinting" type="DeviceFingerPrintingType"
				maxOccurs="1" minOccurs="0">
			</xsd:element>
			<xsd:element name="blacklist_status" type="BlacklistStatusType"
				maxOccurs="1" minOccurs="1">
			</xsd:element>
			<xsd:element name="order_details" type="OrderDetailsType"
				maxOccurs="1" minOccurs="1">
			</xsd:element>
			<xsd:element name="technical_details" type="TechnicalDetailsType"
				maxOccurs="1" minOccurs="1">
			</xsd:element>
			<xsd:element name="applicant_type" type="ApplicantTypeType"
				maxOccurs="1" minOccurs="1">
			</xsd:element>
			<xsd:element name="workflow_name" type="WorkflowNameType"
				maxOccurs="1" minOccurs="1">
			</xsd:element>

			<xsd:element name="merchant_details" type="MerchantDetailsType"
				maxOccurs="1" minOccurs="1"></xsd:element>
			<xsd:element name="payment_history_within_last_three_months"
				type="OrderPaymentHistoryType" maxOccurs="1" minOccurs="1">
			</xsd:element>
			<xsd:element name="payment_history_within_last_twelve_months"
				type="OrderPaymentHistoryType" maxOccurs="1" minOccurs="1">
			</xsd:element>
			<xsd:element name="payment_history_since_ever" type="OrderPaymentHistoryType"
				maxOccurs="1" minOccurs="1">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CompanyDetailsType">
		<xsd:sequence>
			<xsd:element name="name" type="xsd:string" minOccurs="1"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>personal data bank code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="legal_form" type="xsd:string"
				minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>value of shopping basket including shipment
						costs</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="register_number" type="xsd:string"
				minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>currency of shopping basket</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tax_number" type="xsd:string"
				minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>items in cart</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="DeviceFingerPrintingType">
		<xsd:sequence>
			<xsd:element name="exact_id" type="xsd:string" minOccurs="0"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						finger print id
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="policy_score" type="xsd:string"
				maxOccurs="1" minOccurs="0"></xsd:element>
			<xsd:element name="smart_id" type="xsd:string" maxOccurs="1"
				minOccurs="0"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="BillingAddressDataType">
		<xsd:sequence>
			<xsd:element name="email" type="xsd:string" minOccurs="1"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						delivery address additional
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="gender" type="xsd:string" minOccurs="1"
				maxOccurs="1" />
			<xsd:element name="title" type="xsd:string" minOccurs="0"
				maxOccurs="1" />
			<xsd:element name="street_number" type="xsd:string"
				minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						delivery address street number
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="country" type="xsd:string" minOccurs="1"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						delivery address country
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name" type="xsd:string" minOccurs="1"
				maxOccurs="1" />
			<xsd:element name="city" type="xsd:string" minOccurs="1"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						delivery address city name
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="zip" type="xsd:string" minOccurs="1"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						delivery address city postal code (with leading
						zero, if applicable)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="street" type="xsd:string" minOccurs="1"
				maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						delivery address street name
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="first_name" type="xsd:string"
				minOccurs="1" maxOccurs="1" />
			<xsd:element name="birthday" type="xsd:dateTime" maxOccurs="1"
				minOccurs="1">
			</xsd:element>
			<xsd:element name="phone" type="xsd:string" maxOccurs="1"
				minOccurs="0">
			</xsd:element>
			<xsd:element name="address_addition" type="xsd:string"
				maxOccurs="1" minOccurs="0">
			</xsd:element>
			<xsd:element name="cellphone" type="xsd:string"
				maxOccurs="1" minOccurs="0">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>



	<!-- ############################## response Message ############################## -->

	<xsd:complexType name="dataMessageType">
		<xsd:sequence>
			<xsd:element name="blacklist_updates" type="BlacklistUpdatesType"
				minOccurs="1" maxOccurs="1" />
			<xsd:element name="rating_provider_result" type="RatingProviderResultType"
				minOccurs="1" maxOccurs="1" />
			<xsd:element name="rating_result" type="RatingResultType"
				minOccurs="0" maxOccurs="1"></xsd:element>
			<xsd:element name="payment_regulation" type="PaymentRegulationType"
				maxOccurs="1" minOccurs="0"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>



	<!-- ############################## error Message ############################## -->
	<xsd:complexType name="errorMessageType">
		<xsd:annotation>
			<xsd:documentation>Message type to be returned in case of business
				errors</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="category" type="xsd:string" />
			<xsd:element name="code" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>
						acknowledgment error codes must start with
						grderr_ followed by a five digit
						integer code that is indicative of
						the error/message type
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>

	<!-- ############################## other restrictions ############################## -->














